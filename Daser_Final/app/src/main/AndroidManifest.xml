<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="Daser.team.com.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="Daser.team.com.app.Globalvariables_set"
        android:allowBackup="true"
        android:icon="@mipmap/final_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/final_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="Daser.team.com.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Daser.team.com.app.forgot_password" />
        <activity android:name="Daser.team.com.app.Service_seeker_signup" />
        <activity android:name="Daser.team.com.app.Service_provider_signup" />
        <activity
            android:name="Daser.team.com.app.Logon"
            android:label="@string/title_activity_logon"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="Daser.team.com.app.Emailconfirmation" />

        <receiver android:name="Daser.team.com.app.NotificationActionReceiver">
            <intent-filter>
                <action android:name="CONFIRM" />
                <action android:name="CANCEL" />
            </intent-filter>
        </receiver>

        <service
            android:name="Daser.team.com.app.Seller_bgm_Services"
            android:enabled="true"
            android:exported="false"></service>




        <service android:name="Daser.team.com.app.FirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="Daser.team.com.app.FirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>